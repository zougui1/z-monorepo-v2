{
  "name": "@zougui/workout.front-end",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "npm-run-all --parallel dev:css dev:remix",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev --debug",
    "build": "npm-run-all build:css build:remix",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@mui/x-date-pickers": "^6.8.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@remix-run/css-bundle": "^1.15.0",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@zougui/common.math-utils": "^0.4.0",
    "@zougui/common.ms": "^0.4.0",
    "@zougui/common.object-utils": "^0.2.1",
    "@zougui/common.react.context-store": "^0.1.0",
    "@zougui/common.ts-rest.client": "^0.1.2",
    "@zougui/common.ts-rest.core": "^0.2.2",
    "@zougui/common.ts-rest.express": "^0.1.2",
    "@zougui/common.url-utils": "^0.4.0",
    "alova": "^2.6.2",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "isbot": "^3.6.8",
    "luxon": "^3.3.0",
    "qs": "^6.11.2",
    "radash": "^10.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "type-fest": "^3.12.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
